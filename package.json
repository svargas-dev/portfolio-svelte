		"@typescript-eslint/parser": "^5.30.4",
		"autoprefixer": "^10.4.7",
		"cssnano": "^5.1.12",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^7.7.2",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.8.0",
